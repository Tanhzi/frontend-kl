{
  "name": "dslrbooth",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"npm run server\" \"npm run electron:dev\"",
    "server": "node server.js",
    "electron:dev": "wait-on http://localhost:3000 && electron .",
    "build": "vite build",
    "start": "electron .",
    "dist": "npm run build && electron-builder"
  },
  "build": {
    "appId": "com.example.dslrBooth",
    "productName": "dslrBooth",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**",
      "package.json"
    ],
    "asarUnpack": [
      "**/node_modules/@serialport/**"
    ],
    "win": {
      "target": "nsis"
    }
  },
  "dependencies": {
    "@serialport/parser-readline": "^13.0.0",
    "@tanstack/react-query": "^5.90.2",
    "apexcharts": "^5.3.6",
    "bootstrap": "^5.3.3",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "firebase": "^11.6.1",
    "gifshot": "^0.4.5",
    "lottie-react": "^2.4.1",
    "node-webcam": "^0.8.2",
    "qrcode": "^1.5.4",
    "qrcode.react": "^4.2.0",
    "react": "^19.0.0",
    "react-apexcharts": "^1.8.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.1.5",
    "serialport": "^13.0.0",
    "sharp": "^0.34.1",
    "simple-keyboard": "^3.8.89",
    "socket.io-client": "^4.8.1",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "electron": "^35.0.0",
    "electron-builder": "^25.1.8",
    "eslint": "^9.19.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "vite": "^6.1.0",
    "wait-on": "^8.0.3"
  },
  "description": "This template provides a minimal setup to get React working in Vite with HMR and some ESLint rules.",
  "keywords": [],
  "author": "",
  "license": "ISC"
}
